<div class="page-header">
  <%= link_to new_route_point_path, :class => 'btn btn-primary' do %>
    <i class="icon-plus icon-white"></i>
    New Route point
  <% end %>
  <h1>Listing route_points</h1>
</div>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Seqnum</th>
      <th>Lat</th>
      <th>Lon</th>
      <th>Route</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
<% @route_points.each do |route_point| %>
    <tr>
      <td><%= route_point.seqnum %></td>
      <td><%= route_point.lat %></td>
      <td><%= route_point.lon %></td>
      <td><%= route_point.route %></td>
      <td><%= link_to 'Show', route_point %></td>
      <td><%= link_to 'Edit', edit_route_point_path(route_point) %></td>
      <td><%= link_to 'Destroy', route_point, confirm: 'Are you sure?', method: :delete %></td>
    </tr>
<% end %>
  </tbody>
</table>
