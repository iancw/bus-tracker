<div class="page-header">
  <%= link_to route_points_path, :class => 'btn' do %>
    <i class="icon-list-alt"></i>
    Back
  <% end %>
  <%= link_to edit_route_point_path(@route_point), :class => 'btn btn-primary' do %>
    <i class="icon-pencil icon-white"></i>
    Edit
  <% end %>
  <h1>Listing route_points</h1>
</div>

<dl>
  <dt>Seqnum:</dt>
  <dd><%= @route_point.seqnum %></dd>

  <dt>Lat:</dt>
  <dd><%= @route_point.lat %></dd>

  <dt>Lon:</dt>
  <dd><%= @route_point.lon %></dd>

  <dt>Route:</dt>
  <dd><%= @route_point.route %></dd>

</dl>
